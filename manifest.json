{
  "manifest_version": 2,

  "name": "YouTube bypass Age",
  "description": "Bypass age restriction",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://www.youtube.com; object-src 'self'",
  "content_scripts": [ {
   "css": [ "main.css" ],
   "matches": ["<all_urls>"]
} ],
  "browser_action": {
   "default_icon": "images/get_started16.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}